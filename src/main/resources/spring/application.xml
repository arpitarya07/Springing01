<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.3.xsd">

	
	<bean id="basicMessageOfTheDay" class="com.springing.Springing01.service.BasicMessageOfTheDayImpl">
		<property name="message" value="Hi from Arpit"></property>
	</bean>
	<bean id="printer" class="com.springing.Springing01.service.MessagePrinterService">
		<constructor-arg ref="basicMessageOfTheDay"></constructor-arg>
	</bean>
</beans>